@model IEnumerable<BlogOdev.Models.data.Etiket>

<div class="sidebox widget">
    <h3 class="widget-title section-title">Etiketler</h3>
    <ul class="tag-list">
        @foreach (var item in Model)
        {
            <li><a href="/Etiket/Index/@item.EtiketId">@item.EtiketAdi (@item.Makales.Count)</a></li>
        }
        
       
    </ul>
    <!-- /.tag-list -->

</div>
